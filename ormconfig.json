{
   "type":"sqlite",
   "database": "src/database/db.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "src/app/models/**/*.ts"
   ],
   "migrations": [
      "src/migrations/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/app/models",
      "migrationsDir": "src/database/migrations",
      "subscribersDir": "src/subscriber"
   }
}